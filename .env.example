# LMS Environment Configuration
# Copy this file to .env and fill in your API keys

# LLM Provider API Keys (add the ones you want to use)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_API_KEY=

# Default provider (anthropic, openai, or google)
LMS_DEFAULT_PROVIDER=anthropic

# Default model per provider (optional)
LMS_ANTHROPIC_MODEL=claude-sonnet-4-20250514
LMS_OPENAI_MODEL=gpt-4o
LMS_GOOGLE_MODEL=gemini-1.5-pro
